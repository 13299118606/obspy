# Makefile for coordtrans

INCLUDES= \
	-I .

DEPENDS = \
	coordtrans.o \
	xcorr.o \
	recstalta.o \
	pk_mbaer.o \
	filt_util.o \
	arpicker.o

CFLAGS = -g -O3 -arch i386 -arch x86_64 -arch ppc 
CC = gcc

signal.dylib: $(DEPENDS)
	$(CC) $(CFLAGS) -dynamiclib -lm -o signal.dylib $(DEPENDS)

install: signal.so
	cp signal.so ../lib/signal.so

clean:
	find ./ -name "*.o" -exec rm {} \;
	rm -f *.so
